<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta name="google-site-verification" content="YuzG122tLb6CcKkmF6aL8RL_cSWEOCExcnPCeyBO_M4" />
  <!-- title -->
  <title><%= @view_module.title(@view_template) %></title>
  <meta itemprop="name" content="<%= @view_module.title(@view_template) %>">
  <!-- description -->
  <meta name="description" content="<%= @view_module.description(@view_template) %>">
  <meta itemprop="description" content="<%= @view_module.description(@view_template) %>">
  <!-- fonts -->
  <link href="https://fonts.googleapis.com/css?family=Rubik:300,400,500,700,900" rel="stylesheet">
  <!-- style -->
  <link rel="stylesheet" href="<%= Routes.static_path(@conn, "/css/app.css") %>">
  <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
  <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
  <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>

<body class="fix-header">
  <div class="preloader">
    <svg class="circular" viewBox="25 25 50 50">
      <circle class="path" cx="50" cy="50" r="20" fill="none" stroke-width="2" stroke-miterlimit="10" />
    </svg>
  </div>
  <div id="wrapper">
    <nav class="navbar navbar-default navbar-static-top m-b-0">
      <div class="navbar-header">
        <div class="top-left-part">
          <a class="logo" href="Routes.app_path(@conn, :index) %>">
            <b>
              <img src="/images/admin-logo.png" alt="home" class="dark-logo" />
            </b>
            <h1 class="inline hidden-xs">Request Mock</h1>
          </a>
        </div>
        <ul class="nav navbar-top-links navbar-left">
          <li><a href="javascript:void(0)" class="open-close"><i class="ti-menu"></i></a></li>
        </ul>
      </div>
    </nav>
    <div class="navbar-default sidebar" role="navigation">
      <div class="sidebar-nav slimscrollsidebar">
        <div class="sidebar-head">
          <h3>
            <span class="fa-fw open-close"><i class="ti-close ti-menu"></i></span>
            <span class="hide-menu">Navigation</span>
          </h3>
        </div>
        <ul class="nav" id="side-menu">
          <li>
            <a href="<%= Routes.app_path(@conn, :index) %>">
              <i class="ti ti-home"></i><span class="hide-menu"> Homepage</span>
            </a>
          </li>
          <li>
            <a href="<%= Routes.app_path(@conn, :create) %>">
              <i class="ti ti-agenda"></i><span class="hide-menu"> Create new mock</span>
            </a>
          </li>
          <li>
            <a href="<%= Routes.app_path(@conn, :show) %>" rel="nofollow">
              <i class="ti ti-dashboard"></i><span class="hide-menu"> View mocks</span>
            </a>
          </li>
          <li>
            <a href="<%= Routes.app_path(@conn, :howto, "") %>" rel="nofollow">
              <i class="ti ti-help-alt"></i><span class="hide-menu"> How to use</span>
            </a>
          </li>
        </ul>
      </div>
    </div>
    <div id="page-wrapper">
      <div class="container-fluid">
        <div class="row">
           <%= @inner_content %>
        </div>
      </div>
      <footer class="footer text-center"> 2017 &copy; RequestMock.com</footer>
    </div>
  </div>
  <script src="<%= Routes.static_path(@conn, "/js/vendor.js") %>"></script>
  <script src="<%= Routes.static_path(@conn, "/js/app.js") %>"></script>
  <script>
      (function (i, s, o, g, r, a, m) {
        i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
          (i[r].q = i[r].q || []).push(arguments)
        }, i[r].l = 1 * new Date(); a = s.createElement(o),
          m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
      })(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');

    ga('create', 'UA-29099143-5', 'auto');
    ga('send', 'pageview');
  </script>
</body>

</html>
